{
  "manifest_version": 2,
  "name": "Legos to LEGOs",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}